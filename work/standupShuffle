// Fisher Yates shuffle
const shuffle = (array) => {
  const newArray = [...array];
  for(let i = newArray.length - 1; i > 0; i--) {
    const element = newArray[i]
    
    const j = Math.floor(Math.random() * (i+1))
    
    newArray[i] = newArray[j]
    newArray[j] = element
  }

  return newArray;
}