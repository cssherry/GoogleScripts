const getRandomFoodUrl = () => {
  const url = `https://api.unsplash.com/photos/random?collections=12119715&client_id=ACpOVmZHQdIpGM4yus_6TlR3s3XaQcrhbZDD13Q70GA`;

  const data = UrlFetchApp.fetch(url).getContentText();
  const picture = JSON.parse(data);

  return {
    randomFoodUrl: picture.urls.small,
    altText: picture.alt_description,
    description: picture.description,
  };
}

